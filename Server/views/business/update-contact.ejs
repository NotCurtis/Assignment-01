<%- include ../partials/Header.ejs %> 
<%- include ../partials/Main_Nav.ejs %> 



<div class="container">
    <div class="row">
        <div class="col-md-offset-1 col-md-11">
            <h1><%= title %></h1>

            <form class="form" method="POST">
                <div class="form-group">
                    <label for="nameTextField">Name</label>
                        <input type="text" class="form-control" 
                        id='nameTextField'
                        placeholder="Enter Name"
                        name='Name' value="<%= contact.Name %> "
                         required>
                </div>
                <div class="form-group">
                    <label for="authorTextField">Phone Number</label>
                        <input type="text" class="form-control" 
                        id='pNumberTextField'
                        placeholder="Enter Phone Number"
                        name='PNumber' value="<%= contact.PNumber %> "
                        required>
                </div>
                <div class="form-group">
                    <label for="dateTextField">Email</label>
                        <input type="text" class="form-control" 
                        id='emamilTextField'
                        placeholder="Enter Email"
                        name='Email' value="<%= contact.Email %> " required>
                </div>

                <button class="btn btn-primary">
                    <i class="fas fa-edit"></i> Submit</button>
                <a href="/business-contact" class="btn btn-warning">
                    <i class="fas fa-undo"></i> Cancel</a>
            </form>

        </div>
</div>

  
<%- include ../partials/Bottom_Nav.ejs %> 
<%- include ../partials/Footer.ejs %> 